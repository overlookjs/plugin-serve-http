{
  "name": "@overlook/plugin-serve-http",
  "version": "0.1.1",
  "description": "Overlook framework HTTP server plugin",
  "main": "index.js",
  "files": [
    "lib/**/*.js"
  ],
  "author": {
    "name": "Overlook Motel"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/overlookjs/plugin-serve-http.git"
  },
  "bugs": {
    "url": "https://github.com/overlookjs/plugin-serve-http/issues"
  },
  "dependencies": {
    "@overlook/plugin": "^0.2.3",
    "@overlook/plugin-start": "^0.1.2",
    "@overlook/route": "^0.5.1",
    "is-it-type": "^3.1.1"
  },
  "devDependencies": {
    "@overlookmotel/eslint-config": "^7.1.2",
    "@overlookmotel/eslint-config-jest": "^4.0.1",
    "@overlookmotel/eslint-config-node": "^2.0.0",
    "axios": "^0.19.2",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^25.4.0",
    "jest-extended": "^0.11.5"
  },
  "keywords": [
    "overlook",
    "overlook-plugin",
    "serve",
    "server",
    "http"
  ],
  "scripts": {
    "test": "npm run lint && npm run test-main",
    "lint": "eslint '*.js' '.*.js' '**/*.js' '**/.*.js'",
    "lint-fix": "eslint '*.js' '.*.js' '**/*.js' '**/.*.js' --fix",
    "test-main": "jest",
    "test-prod": "cross-env NODE_ENV=production jest",
    "cover": "npm run cover-main && rm -rf coverage",
    "coveralls": "npm run cover-main && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "cover-main": "cross-env COVERAGE=true NODE_ENV=production jest --coverage",
    "ci": "if [ $COVERAGE ]; then npm run coveralls; else npm test; fi"
  },
  "engines": {
    "node": ">=10"
  },
  "readmeFilename": "README.md",
  "license": "MIT"
}
